      },
      "schedule": {
        "ref": "ScheduleId_Qc7ip"
      },
      "insertMode": "OVERWRITE_EXISTING",
      "maximumRetries": "1",
      "runsOn": {
        "ref": "RedshiftLoadEc2Resource"
      },
      "onFail": {
        "ref": "SnsAlarmId218"
      },
      "onSuccess": {
        "ref": "ActionId3"
      },
      "output": {
        "ref": "table_twoRedshiftDataNode"
      },
      "type": "RedshiftCopyActivity"
    },
    {
      "name": "Redshift: table_two",
      "id": "table_twoRedshiftDataNode",
      "schedule": {
